{
  "name": "esa sorter",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Sort categories in esa.io",
  "content_scripts": [
    {
      "matches": ["https://*.esa.io/*"],
      "js": ["lib/Sortable.min.js", "src/script.js"],
      "css": ["css/style.css"]
    }
  ],
  "permissions": ["storage"]
}
